<h1>Belleza</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Direccion</th>
    <th>Descripción</th>
    <th>Reseña</th>
    <th>Photo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @businesses.each do |business| %>
  <tr>
    <td><%= business.name %></td>
    <td><%= business.address %></td>
    <td><%= business.descrption %></td>
    <td><%= business.review %></td>
    <td><%= business.photo %></td>
    <td><%= link_to 'Show', business %></td>
    <td><%= link_to 'Edit', edit_business_path(business) %></td>
    <td><%= link_to 'Destroy', business, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

