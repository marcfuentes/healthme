<div class="container">
<%= simple_form_for(@clinic) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :information %>
    <%= f.input :address %>
    <%= f.file_field :picture %>
    <%= f.input :longitude %>
    <%= f.input :latitude %>
    <%= f.input :gmaps %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
</div>