<%= simple_form_for(@doctor) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :doctor_id %>
    <%= f.input :speciality %>
    <%= f.input :name %>
    <%= f.input :practice %>
    <%= f.input :declaration %>
    <%= f.input :education %>
    <%= f.input :affiliations %>
    <%= f.input :lenguague %>
    <%= f.input :certifications %>
    <%= f.input :memberships %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
