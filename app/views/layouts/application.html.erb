  <!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= content_for?(:title) ? yield(:title) : "Healthme" %></title>
    <meta name="description" content="<%= content_for?(:description) ? yield(:description) : "Healthme" %>">
    <%= stylesheet_link_tag "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%= yield(:head) %>
  </head>
  <body class="<%= controller_name %> <%= action_name %>">
    <div class="navbar navbar-fixed-top">
      <nav class="navbar-inner">
        <div class="container">
          <a class="brand" href="http://localhost:3000"> <img src="http://img.photobucket.com/albums/v234/marfue179/logonavbar_zps051324af.jpg" alt="Health_Me_logo_transparent"></a>
          <%= render 'layouts/navigation' %>
           <ul class="nav">
             <li><%= link_to "Medics", "/medics" %></li>
             <li><%= link_to "Cities", "/cities" %></li>
           </ul>
        </div>
      </nav>
    </div>
    
    
<%= yield %>
<%= render 'layouts/footer' %>
<%= yield :scripts %>

  </body>
</html>
