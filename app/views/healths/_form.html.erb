<%= simple_form_for(@health) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :speciality %>
    <%= f.input :address %>
    <%= f.input :declaration %>
    <%= f.input :education %>
    <%= f.input :review %>
    <%= f.input :photo %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
